extends layout

block content
	.container-fluid    
		.row
			h4.col-md-4.col-md-offset-4.text-center #{user.displayName}					
		.row.classes.text-center
			.h4 Classes
			.class-list				
				each studentClass in classes
					span.class-name #{studentClass.name}
		.row.create-class.text-center
			a.btn.btn-primary(href="/classes/new") create new class
